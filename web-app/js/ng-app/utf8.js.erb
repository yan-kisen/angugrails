App.config(['$httpProvider', function ($httpProvider) {
  $httpProvider.defaults.transformRequest.push(function (data, headersGetter) {
    utf8_data = data;
    if (!angular.isUndefined(data)) {
      d = angular.fromJson(data);
      d['_utf8'] = '&#9731;';
      utf8_data = angular.toJson(d);
    }
    return utf8_data;
  });
}]);